{
  "name": "AirDash",
  "short_name": "AirDash",
  "description": "One click file transfer from Android to Computer",
  "start_url": "/app/index.html",
  "display": "standalone",
  "theme_color": "#ffffff",
  "background_color": "#ffffff",
  "share_target": {
    "enctype": "multipart/form-data",
    "method": "POST",
    "action": "/app/index.html",
    "params": {
      "title": "title",
      "text": "text",
      "rawtext": "rawtext",
      "url": "url",
      "files": [
        {"name": "file", "accept": ["*/*"]}
      ]
    }
  },
  "icons": [
    {
      "src": "logo.png",
      "sizes": "512x512",
      "type": "image/png"
    }
  ]
}
