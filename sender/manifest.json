{
  "name": "AirDash",
  "short_name": "AirDash",
  "description": "One click file transfer from Android to Computer",
  "start_url": "./index.html",
  "display": "standalone",
  "theme_color": "#ffffff",
  "background_color": "#ffffff",
  "share_target": {
    "enctype": "multipart/form-data",
    "method": "POST",
    "action": "./index.html",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url",
      "files": [
        {"name": "file", "accept": ["text/html", ".html"]},
        {"name": "file", "accept": ["text/html", ".htm"]},
        {"name": "file", "accept": ["text/plain", ".txt"]},
        {"name": "file", "accept": ["application/pdf", ".pdf"]},
        {"name": "imaging", "accept": ["image/png", ".png"]},
        {"name": "imaging", "accept": ["image/jpeg", ".jpg"]},
        {"name": "imaging", "accept": ["image/jpeg", ".jpeg"]},
        {"name": "file", "accept": ["text/csv", ".csv"]}
      ]
    }
  },
  "icons": [
    {
      "src": "logo.png",
      "sizes": "512x512",
      "type": "image/png"
    }
  ]
}
