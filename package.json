{
  "private": true,
  "productName": "AirDash",
  "main": "./receiver/main.js",
  "dependencies": {
    "electron-is-dev": "^1.1.0",
    "material-components-web": "^4.0.0",
    "menubar": "^7.2.0",
    "peerjs": "^1.2.0"
  },
  "devDependencies": {
    "electron": "^7.1.9",
    "electron-packager": "^14.2.0",
    "live-server": "^1.2.1",
    "prettier": "^1.19.1"
  },
  "prettier": {
    "trailingComma": "es5",
    "semi": false,
    "singleQuote": true
  },
  "scripts": {
    "start": "electron . --dev",
    "start-sender": "live-server . --open=/sender",
    "build-receiver": "npx electron-packager . --overwrite",
    "deploy-sender": "rsync -a -e 'ssh -p 3333' sender/ simonb@flown.io:/var/www/flown.io/airdash --stats --progress",
    "test": "echo \"Error: no test specified\" && exit 1"
  }
}
